pipeline{
    agent any
    triggers {
        pollSCM("* * * * *")
    }
    stages{
        stage("deliverToDockerHub") {
            steps {
                echo "delivering to docker hub (coming soon)"
                
            }
        }

        stage("Build1"){
            steps{
                echo "========Build========"
                sh "dotnet build"
            }
        }

        stage ("run"){
            steps{
                echo "========Run========"
                sh "dotnet run --project MassasoftApi/MassasoftApi.csproj"
            }
        }
    }
}